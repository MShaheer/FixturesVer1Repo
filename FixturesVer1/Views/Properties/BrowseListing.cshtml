@{
    ViewBag.Title = "BrowseListing";
}



@using (Html.BeginForm("BrowseListing", "Properties"))
{
    @Html.AntiForgeryToken()

    <div class="col-lg-6">
    
    <div class="panel panel-default">
        <div class=" panel-heading">
        <h3 class="panel-title">Browse</h3>
    </div>
        <div class="panel-body">
            <div class="form-horizontal">
                <h4>Properties</h4>
                @Html.ValidationSummary(true)

                <div class="row">
                 
                    <div class="col-lg-6">

                        <div class="form-group">
                            @*  @Html.LabelFor(model => model.Name, new { @class = "control-label col-md-2" })*@
                            <div class="col-md-10">
                                <div class="input-group">
                                    <span class="input-group-addon">Properties</span>
                                    @Html.DropDownList("propertyId", (List<SelectListItem>)ViewBag.propertiesList, new { @class = "form-control" })
                              
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-md-10">
                        <input type="submit" value="Submit" class="btn btn-default" />
                    </div>
                </div>

            </div>
        </div>
    </div>

        </div>

}